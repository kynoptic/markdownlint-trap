{
  "name": "markdownlint-trap",
  "version": "1.5.0",
  "main": "./src/index.js",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf .markdownlint-rules",
    "build": "BABEL_ENV=build babel src --out-dir .markdownlint-rules --extensions '.js,.mjs' --out-file-extension .cjs --copy-files",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration",
    "test:performance": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration/performance.test.js",
    "test:integration:external": "node --experimental-vm-modules node_modules/jest/bin/jest.js tests/integration/curated-repos.test.js --verbose",
    "lint": "eslint .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.7",
    "@babel/preset-env": "^7.27.2",
    "@types/jest": "^29.5.14",
    "babel-jest": "^30.0.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-transform-rewrite-imports": "^1.5.3",
    "debug": "^4.4.1",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "markdown-it": "^14.1.0",
    "markdownlint": "^0.38.0",
    "markdownlint-cli2": "^0.18.1",
    "prettier": "^3.6.2"
  },
  "engines": {
    "node": ">=14"
  }
}
