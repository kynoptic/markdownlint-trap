{
  "presets": [
    ["@babel/preset-env", { "targets": { "node": "current" } }]
  ],
  "env": {
    "build": {
      "plugins": [
        "add-module-exports",
        ["babel-plugin-transform-rewrite-imports", {
          "replaceExtensions": {
            ".js": ".cjs"
          }
        }]
      ]
    }
  }
}
